<canvas id="<%= stat.id %>" width="400" height="200"></canvas>
<script>
var ctx = document.getElementById("<%= stat.id %>");
var myChart = new Chart(ctx, {
    type: '<%= stat.chartType %>',
    data: {
        labels: ["<%-stat.keys.join('","')%>"],
        datasets: [{
            label: "<%= stat.label %>",
            data: [<%= stat.values %>],
            backgroundColor: [
                'rgba(0, 0, 125, 0.5)',
                'rgba(255, 99, 132, 0.5)',
                'rgba(255, 206, 86, 0.5)',
                'rgba(54, 162, 235, 0.5)'
                
            ],
            borderColor: [
                'rgba(0, 0, 125, 1)',
                'rgba(255,99,132,1)',
                'rgba(255, 206, 86, 1)',
                'rgba(54, 162, 235, 1)',
            ],
            borderWidth: 1
        }]
    },
    options: {
        responsive: true,
    }
});
</script>